package algoritOrdenacao;

import java.util.List;

public class QuickSort {
	
	public QuickSort() {
		// TODO Auto-generated constructor stub
	}
	
	public void quickSort(List<Integer> listInteiros, int inicio, int fim){
		if(inicio < fim){
			int index = particionaLista(listInteiros, inicio, fim);
			quickSort(listInteiros, inicio, index-1);
			quickSort(listInteiros, index+1, fim);
		}
	}

	private int particionaLista(List<Integer> listInteiros, int inicio, int fim) {
		// TODO Auto-generated method stub
		return 0;
	}
	
}
